export FLASK_ENV=development
export FLASK_APP=app.py
export OCO_LANGUAGE=en
export KUBE_ENV=development
# export CUSTOM_CERT=certs
export LOG_FORMAT=text
export LOG_LEVEL=INFO

# Autoswagger configuration
export ENABLE_AUTOSWAGGER=true
export AUTOSWAGGER_RATE_LIMIT=30
export AUTOSWAGGER_TIMEOUT=10
export AUTOSWAGGER_MAX_CONCURRENT=5
export DISABLE_PRESIDIO=false
export SWAGGER_DISCOVERY_INTERVAL=600  # 10 minutes en dev (vs 1h en prod)