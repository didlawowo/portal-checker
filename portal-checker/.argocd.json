{
    "source": {
        "repoURL": "git@github.com:didlawowo/continuous-delivery.git",
        "targetRevision": "main",
        "path": "charts/portal-checker"
    },
    "destination": {
        "namespace": "portal"
    },
    "syncPolicy": {
        "syncOptions": {
            "CreateNamespace": true
        }
    },
    "clusters": {
        "develop": {
            "enabled": true
        },
        "preprod": {
            "enabled": true,
            "valuesRevision": "main"
        },
        "in-cluster": {
            "enabled": true,
            "valuesRevision": "main"
        }

    }
    
}
